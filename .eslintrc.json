{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "rules": {
    "multiline-ternary": 0,
    "react/no-direct-mutation-state": 0,
    "react/no-find-dom-node": 0,
    "no-unused-vars": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["prettier"],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "NodeJS": true,
    "RPC": true
  }
}
